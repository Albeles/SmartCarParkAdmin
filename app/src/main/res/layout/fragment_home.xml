<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="500dp"
    tools:context=".ui.ParkingLotAFragment">


    <Button
        android:id="@+id/parkinglota"
        android:layout_width="270dp"
        android:layout_height="65dp"
        android:layout_marginTop="150dp"
        android:text="@string/parkinglotaa"
        android:textAllCaps="false"
        android:textSize="24sp"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/parkinglotb"
        android:layout_width="270dp"
        android:layout_height="65dp"
        android:layout_marginTop="100dp"
        android:text="@string/parkinglotbb"
        android:textAllCaps="false"
        android:textSize="24sp"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/parkinglota" />
</androidx.constraintlayout.widget.ConstraintLayout>